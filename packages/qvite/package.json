{
  "name": "@quiteer/vite",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@10.20.0",
  "description": "在 vite 的基础上增强的前端架构 cli",
  "author": "quiteer",
  "license": "MIT",
  "homepage": "https://quiteerjs.github.io/web/cli/qvite.html",
  "keywords": [
    "vite",
    "cli"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "client": [
        "./client.d.ts"
      ],
      "*": [
        "./src/*"
      ]
    }
  },
  "bin": {
    "qvite": "./dist/bin/qvite.mjs"
  },
  "files": [
    "client.d.ts",
    "dist"
  ],
  "scripts": {
    "dev": "tsdown -w",
    "build": "tsdown",
    "release": "qui r --tag-prefix qvite && tsdown && pnpm publish"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@quiteer/is": "workspace:*",
    "@quiteer/parser-config": "^1.0.3",
    "@quiteer/unocss": "workspace:*",
    "@quiteer/utils": "workspace:*",
    "@quiteer/vite-plugins": "workspace:^",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "c12": "3.3.2",
    "cac": "6.7.14",
    "fs-extra": "^11.3.2",
    "kolorist": "1.8.0",
    "portfinder": "^1.0.38",
    "semver": "^7.6.3",
    "tsdown": "^0.16.1",
    "vite": "npm:rolldown-vite@7.2.2",
    "zx": "^8.8.5"
  }
}
