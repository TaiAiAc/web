{
  "name": "@quiteer/unocss",
  "version": "0.0.0",
  "description": "UnoCSS 集成插件与统一配置集合，面向 Vite 项目提供即插即用的预设组合、主题、规则与快捷类。",
  "homepage": "https://quiteerjs.github.io/web/plugins/unocss/index.html",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": ["dist"],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "release": "qui r --tag-prefix unocss && tsdown && pnpm publish --access public"
  },
  "peerDependencies": {
    "@unocss/vite": "^66.5.10",
    "vite": ">=5"
  },
  "dependencies": {
    "@iconify/json": "2.2.405",
    "@unocss/preset-web-fonts": "^66.5.10",
    "@unocss/preset-wind4": "^66.5.10",
    "@unocss/transformer-directives": "^66.5.10",
    "@unocss/transformer-variant-group": "^66.5.10",
    "unocss": "^66.5.10"
  }
}
