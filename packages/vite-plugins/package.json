{
  "name": "@quiteer/vite-plugins",
  "type": "module",
  "version": "0.1.2",
  "description": "Vite 插件集合，旨在提供开箱即用的开发体验，涵盖环境配置、Mock、HTML 处理及常用插件封装。",
  "author": "",
  "license": "MIT",
  "homepage": "https://quiteerjs.github.io/web/plugins/vite-plugin/",
  "keywords": [
    "vite",
    "plugin"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./obfuscation": {
      "types": "./dist/obfuscation.d.ts",
      "import": {
        "types": "./dist/obfuscation.d.ts",
        "default": "./dist/obfuscation.js"
      }
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown -w",
    "build": "tsdown",
    "release": "qui r --tag-prefix vite-plugins && tsdown && pnpm publish"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@quiteer/utils": "workspace:*",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "c12": "3.3.2",
    "dotenv": "^17.2.3",
    "fast-glob": "^3.3.3",
    "kolorist": "1.8.0",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^8.0.3"
  }
}
